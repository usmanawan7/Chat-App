<%= form_with(model: @room, remote: true, class: "d-flex",
     data: { controller: "reset-form", action: "turbo:submit-end->reset-form#reset" }) do |f| %>
  <%= f.text_field :name, class: "form-control", autocomplete: 'off' %>
  <%= f.submit "Create Room", data: { disable_with: false }, class: "btn fa-lg gradient-custom-2 mb-3 ml-1"%>
<% end %>
